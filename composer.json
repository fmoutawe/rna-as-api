{
  "name": "ivelfan/rna-as-api",
  "type": "library",
  "description": "Call RNA API services to find informations about association and company",
  "keywords": [
    "rna",
    "siret",
    "association",
    "company"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flavien Moutawe"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.0",
    "ronanguilloux/isocodes": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "RNA\\": "src/RNA/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RNA\\Test\\": "test/RNA/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
